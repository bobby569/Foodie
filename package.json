		"react-test-renderer": "^16.2.0",
		"selenium-webdriver": "3.0.0-beta-2"
	},
	"jest": {
		"coverageReporters": ["html", "text"],
		"setupTestFrameworkScriptFile": "<rootDir>/imports/test/config_test.js",
		"moduleNameMapper": {
			"^meteor/(.*)": "<rootDir>/.meteorMocks/index.js"
