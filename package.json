		"lodash": "4.17.5",
		"meteor-node-stubs": "^0.3.2",
		"omit.js": "^1.0.0",
		"prop-types": "^15.6.1",
		"rc-calendar": "^9.6.0",
		"rc-pagination": "^1.15.1",
		"rc-select": "^7.7.1",
		"rc-util": "^4.4.0",
		"react": "^16.2.0",
		"react-addons-pure-render-mixin": "^15.6.2",
		"react-bootstrap": "^0.32.1",
		"react-dom": "^16.2.0",
		"react-dropzone": "4.2.8",
